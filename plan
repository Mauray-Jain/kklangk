stack based
(whitespace inspired)
grammar: asa, chuss, yeh, tuh, saade, belle, horr, goiz,  , \n

0 = yeh
1 = tuh

stack = asa
arithmetic = horr
heap = saade
flow = belle
io = goiz

stack:
	push = chuss (num)
	dup = asa
	copynth = saade (num)
	swap = yeh
	pop = tuh

arithmetic:
	add = tuh
	sub = yeh
	mult = asa
	div = chuss
	mod = horr
	xor = goiz

heap:
	str = yeh
	ret = tuh

flow:
	mark = asa (num)
	call = chuss (num)
	jmp = horr (num)
	jmpif0 = goiz (num)
	jmpifneg = saade (num)
	ret = yeh
	exit = tuh

io:
	outc = yeh
	inc = tuh
	outd = asa
	ind = chuss
